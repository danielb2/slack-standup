{
  "name": "slack-standup",
  "version": "1.0.0",
  "description": "Slack standup script. Uses nice formatting to post a daily status to a slack channel.",
  "main": "index.js",
  "scripts": {
    "rename": "find ~/.standup -depth -type f -name \"*.txt\" -exec sh -c 'mv \"$1\" \"${1%.txt}.json\"' _ {} \\;",
