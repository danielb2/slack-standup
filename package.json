      "url": "https://github.com/danielb2/"
    }
  ],
  "license": "ISC",
  "dependencies": {
    "@hapi/joi": "^15.1.1",
    "@slack/web-api": "^5.11.0",
    "fs-extra": "^8.1.0",
    "js-yaml": "^3.13.1",
    "json5": "^2.1.0"
  },
  "bin": {
    "slack-standup": "slack-standup.js"
  }
}
